    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "googleapis": "^48.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "nock": "^12.0.3",
    "node-mocks-http": "^1.8.1"
  }
