{
  "name": "micro-auth-request",
  "version": "0.1.4",
  "description": "Simple & fast authentication for Kubernetes ingress using Google OAuth2",
  "main": "lib/app.js",
  "scripts": {
    "start": "node lib/app.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
